include /components/checkbox/checkbox

mixin checkboxList(customConfig = {})
    - const defaultConfig = require("@components/checkbox-list/checkbox-list.config.json");
    - const mergedConfig = Object.assign({}, defaultConfig, customConfig);
    - const { title, list: items, isOpen } = mergedConfig;
    - let className = "checkbox-list";
    - className += isOpen ? " open" : "";
    div(class=className)
        h3.checkbox-list__title= title
        ul.checkbox-list__items
            each val in items
                li.checkbox-list__item 
                    +checkbox(val)
